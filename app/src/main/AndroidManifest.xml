<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timemanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TimeManager">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.projects.ProjectDetailActivity" />
        <activity android:name=".ui.tasks.TaskActivity" />
        <activity android:name=".ui.projects.ProjectActivity" />
        <activity android:name=".ui.tasks.TaskDetailActivity" />
        <activity android:name=".ui.info.OverviewActivity" />
        <activity android:name=".ui.info.BookingListActivity" />
        <activity android:name=".ui.info.WorkingHoursDailyActivity" />
        <activity android:name=".ui.info.WorkingHoursWeeklyActivity" />
    </application>

</manifest>